- var titlePage = 'Inspection';
doctype html
html(lang="en")
  head
    include includes/head.pug
    //- link(rel="stylesheet", href="../assets/css/jquery-steps.css")
  body 
    .Dashboard-home
      include includes/headerNavBar.pug
      include includes/asideBar.pug
      //- start Content category hotel
      section.content-category-hotel 
        //- Start Filter
        include includes/FilterDashboard.pug
        //- End Filter
        //- Start Load Master page 
        #pageMasterLoad
          //- Start Create Order
          .Create-Order
            .container-fluid 
              .row 
                .col-lg-12 
                  //- Start Create Order
                  .Create-Order
                    .container-fluid 
                      .row 
                        .col-lg-12 
                          .button-room.card-master.setup-Room-page
                            .container-fluid 
                              .row 
                                .col-lg-6.col-md-6.col-sm-12 
                                  .room-information.mb-5
                                    h5.mb-4 Locations
                                    .room-name.mb-3 
                                      .icon 
                                        i.fa-solid.fa-pencil.fa-fw
                                      .text Room
                                    button.btn.d-flex
                                      .icon.me-2 
                                        i.fa-solid.fa-plus
                                      .text Categories
                                    ul.list-unstyled.list-categories.mb-3
                                      li.d-flex.active 
                                        .text General
                                      li.d-flex 
                                        .text Bathroom
                                      li.d-flex 
                                        .text Bathroom

                                .col-lg-6.col-md-6.col-sm-12
                                  h4.text-center.text-primary Add Questions For General Category of Rooms.
                                  .form-group.mb-4
                                    label(for="") Questions
                                    .input-group.mb-3
                                      select.form-select(aria-label='Default select example')
                                        option(selected='') Are the windows and mirrors clean?
                                        option(value='1') One
                                        option(value='2') Two
                                        option(value='3') Three

                                      span#basic-addon2.input-group-text.btn-outline-master 
                                        i.fa-solid.fa-plus
                                  
                                  .form-group.mb-4
                                    label(for="") Add Answers
                                    .input-group.mb-3
                                      select.form-select(aria-label='Default select example')
                                        option(selected='') Yes
                                        option(value='1') No

                                      span#basic-addon2.input-group-text.btn-outline-master 
                                        i.fa-solid.fa-plus
                                  .form-group.mb-4
                                    label(for="") Added Answers
                                    .d-flex 
                                      input#option5.btn-check(type='radio', name='options-base', autocomplete='off', checked='')
                                      label.btn(for='option5') Yes
                                      input#option6.btn-check(type='radio', name='options-base', autocomplete='off')
                                      label.btn(for='option6') No
                                  .form-group.mb-4
                                    h5.mb-4 Department
                                    .group-button 
                                      span.btn.mx-2.rounded-pill.btn-outline-master.mb-3.active Maintenance
                                      span.btn.mx-2.rounded-pill.btn-outline-master.mb-3 Reception
                                      span.btn.mx-2.rounded-pill.btn-outline-master.mb-3 Delivery
                                      span.btn.mx-2.rounded-pill.btn-outline-master.mb-3 Procurement
                                  .form-group.mb-4
                                    h5.mb-4 Assignee
                                    .label-Assignee
                                      .icon 
                                        span R
                                      .text Rohana
                                  button.btn.d-flex.mb-4
                                      .icon.me-2 
                                        i.fa-solid.fa-plus
                                      .text Add Question
                                  .d-flex.justify-content-center.w-100    
                                    a.btn.btn-master.rounded-pill.text-center.w-50(href="inspectionFinish.html")  Continue


                                
          //- End Create Order
                  
   
            


      //- End Content category hotel
    //- Start Modal
    include includes/modal.pug 
    include includes/script.pug
